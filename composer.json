{
	"name": "v12mike/sitemap",
	"type": "phpbb-extension",
	"description": "Creates an XML sitemap for a phpBB forum. You can access the sitemap on {MY_BOARD_URL}/sitemap.xml or {MY_BOARD_URL}/app.php/sitemap.xml.",
	"homepage": "",
	"version": "0.2.1",
	"keywords": ["phpbb", "extension", "seo", "sitemap"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "tas2580",
			"email": "mail@tas2580.net",
			"homepage": "https://tas2580.net",
			"role": "Developer"
		},
		{
			"name": "v12mike",
			"email": "morrinmike@gmail.com",
			"homepage": "",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.0.0",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "XML Sitemap",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<3.4.0@dev"
		},
		"version-check": {
			"host": "frenchcarforum.co.uk",
			"directory": "/versions",
			"filename": "sitemap.json"
		}
	}
}
